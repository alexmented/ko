<p id="notice">
    <%= notice %>
</p>
<h1><%= t :users%></h1>

<table>
  <thead>
    <tr>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.email %></td>
        <td><%= link_to t(:show), user %></td>
        <td><%= link_to t(:edit), edit_user_path(user) %></td>
        <td><%= link_to t(:delete), user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to t(:newUser), new_user_path %>
